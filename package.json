  "scripts": {
    "dev": "next dev",
    "webcontainer:dev": "NODE_ENV=development NEXT_PUBLIC_ENV_MODE=webcontainer env-cmd --no-override -f .env.webcontainer next dev",
    "build": "next build",
    "build:staging": "env-cmd -f .env.staging next build",
    "build:production": "env-cmd -f .env.production next build",
    "build:webcontainer": "env-cmd -f .env.webcontainer next build",
    "start": "next start",
    "start:staging": "env-cmd -f .env.staging next start",
    "start:production": "env-cmd -f .env.production next start",
    "start:webcontainer": "env-cmd -f .env.webcontainer next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "migrate": "ts-node -r tsconfig-paths/register --project tsconfig.json lib/db/migrations/migrate.ts",
    "test": "jest",
    "webcontainer:setup": "npm install && env-cmd -f .env.webcontainer npm run migrate",
    "webcontainer:start": "npm run webcontainer:setup && npm run webcontainer:dev",
    "test-db": "ts-node test-db.ts"
